{
  "manifest_version": 2,
  "name": "New tab",
  "short_name": "Tab",
  "version": "2.0.5",
  "description": "A custom version of tabliss",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "newTab@wouterdb.nl"
    }
  },
  "icons": {
    "32": "icons/32.png",
    "48": "icons/48.png",
    "96": "icons/96.png",
    "128": "icons/128.png"
  },
  "permissions": ["storage"]
}
